{
  "expo": {
    "name": "Sookari",
    "description": "A modern marketplace app for buying and selling products.",
    "slug": "sookari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sookari",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.sookari.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.sookari.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f125300-eda3-46c6-a470-5495bca9a558"
      }
    },
    "owner": "albatros01"
  }
}
